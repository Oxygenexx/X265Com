@{
    ViewBag.Title = "Home";
}

<div class="jumbotron">
    <h1>x265Com</h1>
    <p class="lead">x265Com is an online HEVC encoder</p>
</div>

<div class="row">
    <div class="col-md-4">
        <h2>Getting started</h2>
        Profile de transcode :<br>
        <p>
            <select name="Profile de transcode">
                <option value="1920*1080">Android</option>
                <option value="Ipad">Ipad</option>
                <option value="Youtube">Youtube </option>
            </select>
            
        </p>        
        <p>
            <input type="checkbox" name="Wpp" value="isWpp"> Use Wpp
        </p>
        <p>
            <select name="ConversionSpeed/Quality">
                <option value="Slow">Slow</option>
                <option value="Regular">Regular</option>
                <option value="Fast">Fast</option>
            </select>
            Conversion Speed/Quality
        </p>
        <p>
            <input type="checkbox" name="QP" value="isQP"> Use Quantizer Parameter

        </p>


        <p><button type="button" onclick="DoSomething" class="btn btn-primary btn-lg">Begin Conversion &raquo;</button></p>
    </div>
</div>